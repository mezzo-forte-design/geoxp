{
  "name": "@geoxp/core",
  "version": "1.0.0",
  "description": "Mezzo Forte GeoXp Core module",
  "homepage": "https://gitlab.com/mezzo-forte/geoxp#README",
  "bugs": {
    "url": "https://gitlab.com/mezzo-forte/geoxp/-/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/mezzo-forte/geoxp.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Giuliano Buratti",
      "email": "gbcode.it@gmail.com"
    },
    {
      "name": "Francesco Cretti",
      "email": "francesco.cretti@gmail.com"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "browser": "dist/geoxp-core.min.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:clear && rollup -c --bundleConfigAsCjs && npm run cjs && npm run esm",
    "build:clear": "rm -rf dist && rm -rf .rollup.cache",
    "cjs": "cd dist && mkdir -p cjs && mv index.cjs index.cjs.map cjs/",
    "esm": "cd dist && mkdir -p esm && mv index.js index.js.map index.d.ts src esm/",
    "lint": "eslint --fix index.ts ./src",
    "lint:test": "eslint index.ts ./src",
    "prettify": "prettier --write \"./src/**/*.{ts,tsx}\" \"index.ts\"",
    "prettify:test": "prettier --list-different \"./src/**/*.{ts,tsx}\" \"index.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@geoxp/utils": "^1.0.0"
  },
  "devDependencies": {
    "@geoxp/eslint-config": "*",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
